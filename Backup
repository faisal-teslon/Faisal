#!/bin/bash

mkdir ctakes-resources-4.0-bin
cd ctakes-resources-4.0-bin
wget http://sourceforge.net/projects/ctakesresources/files/ctakes-resources-4.0-bin.zip
unzip ctakes-resources-4.0-bin.zip
sed -i 's+jdbc:hsqldb:file:resources/org/apache/ctakes/dictionary/lookup/fast/sno_rx_16ab/sno_rx_16ab+jdbc:hsqldb:file:org/apache/ctakes/dictionary/lookup/fast/sno_rx_16ab/sno_rx_16ab+g' resources/org/apache/ctakes/dictionary/lookup/fast/sno_rx_16ab.xml
rm LICENSE
rm README
rm ctakes-resources-4.0-bin.zip


